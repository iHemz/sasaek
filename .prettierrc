{
  "printWidth": 100,
  "trailingComma": "es5",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    ".*styles.css$",
    "",
    "dayjs",
    "^react$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@docs/(.*)$",
    "^@/.*$",
    "^../(?!.*.css$).*$",
    "^./(?!.*.css$).*$",
    "\\.css$"
  ],
  "overrides": [
    {
      "files": "*.mdx",
      "options": {
        "printWidth": 70
      }
    }
  ]
}
